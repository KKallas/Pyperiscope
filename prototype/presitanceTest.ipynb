   "source": [
    "from IPython.display import display, clear_output\n",
    "import asyncio\n",
    "from datetime import datetime\n",
    "\n",
    "async def print_time():\n",
    "    while True:\n",
    "        clear_output(wait=True)\n",
    "        display(datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\"))\n",
    "        await asyncio.sleep(10)\n",
    "\n",
    "await print_time()"
   ]
